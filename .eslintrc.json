{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "jsx-a11y/label-has-for": 1,
    "jsx-a11y/label-has-associated-control": 1,
    "linebreak-style": 0
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ]
}
